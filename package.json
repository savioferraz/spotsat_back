{
  "name": "spotsat_back",
  "version": "1.0.0",
  "description": "Back-end da aplicação Spotsat",
  "main": "index.js",
  "homepage": "https://github.com/savioferraz/spotsat_back#readme",
  "scripts": {
    "build": "npx tsc",
    "db:dump": "sudo su -c 'pg_dump spotsat --inserts --no-owner' postgres > dump.sql",
    "dev:load-envs": "dotenv -e .env.development",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "prisma:create-schema": "npm run dev:load-envs prisma db pull",
    "prisma:generate-client": "npx prisma generate",
    "prisma:migration-generate": "npm run dev:load-envs prisma migrate dev",
    "prisma:migration-run": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "start": "npx nodemon src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savioferraz/spotsat_back.git"
  },
  "author": "savioferraz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savioferraz/spotsat_back/issues"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-boundaries": "^3.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@types/axios": "^0.14.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.6.2",
    "joi": "^17.7.0",
    "ntl": "^5.1.0",
    "pg": "^8.8.0",
    "uuid": "^9.0.0"
  }
}
